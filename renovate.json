{
  "extends": [
    "config:base"
  ],
  "automerge": true,
  "major": {
    "automerge": false
  },
  "prConcurrentLimit": 3,
  "prHourlyLimit": 2,
  "schedule": [
    "after 2am and before 3am on saturday"
  ],
  "updateNotScheduled": false,
  "timezone": "America/New_York",
  "lockFileMaintenance": {
    "enabled": true
  },
  "separatePatchReleases": true,
  "separateMultipleMajor": true,
  "masterIssue": true,
  "labels": [
    "type: dependencies",
    "renovate"
  ],
  "packageRules": [
    {
      "packageNames": [
        "cypress"
      ],
      "groupName": "cypress",
      "schedule": "before 2am"
    }
  ]
}
